proceso ejercicio51
	
	Definir n, suma, elemento, i Como Entero;
    Escribir "Ingrese la cantidad de elementos:";
    Leer n;
    suma <- 0;
	
    Para i <- 1 Hasta n Hacer;
        Escribir "Ingrese el elemento ", i, ":";
        Leer elemento;
        suma <- suma + elemento;
    Fin Para
	
    Escribir "La suma de los elementos es:", suma;
	
Finproceso
